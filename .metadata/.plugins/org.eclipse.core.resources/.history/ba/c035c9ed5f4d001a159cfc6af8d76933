import java.util.HashMap;
import java.util.Scanner;

public class Payroll {
	public static void main(String[] args)
	{
		HashMap<Integer,String>p= new HashMap<Integer,String>();
		p.put(101, "Accountant");
		p.put(102,"Manager");
		p.put(103, "clerk");
		p.put(104, "Accountant");
		p.put(105,"Manager");
		System.out.println(p);
		HashMap<Integer,Integer>q= new HashMap<Integer,Integer>();
		q.put(101,5000);
		q.put(102,8000);
		q.put(103,2000);
		q.put(104,500);
		q.put(105,9000);
		System.out.println(q);
		/*Integer temp = p.get(102);
		temp++;
		p.put(102, temp);*/
		q.put(102, q.get(102) +5000);
		q.put(105, q.get(105) +5000);
		System.out.println(q);
		
	}

}
